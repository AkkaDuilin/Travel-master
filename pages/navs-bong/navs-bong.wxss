.mask{height:70rpx;width:70rpx;background-color:#fcfcfc;opacity:0.9;z-index:2048;border-radius:750rpx;transform:scale(0);}.mask-container{width:100%;height:calc(134rpx + env(safe-area-inset-bottom));display:flex;justify-content:center;align-items:center;position:fixed;z-index:2048;bottom:0;}.maskOpen{animation:maskO 0.3s both;}.maskClose{animation:maskC 0.3s both;}@keyframes maskO{0%{transform:scale(0);}100%{transform:scale(50);}}@keyframes maskC{0%{transform:scale(50);}100%{transform:scale(0);}}.btn-container{width:100%;height:100rpx;margin:0 auto;display:flex;justify-content:center;align-items:center;position:fixed;z-index:2048;bottom:calc(34rpx + env(safe-area-inset-bottom));}.btn-main{border-radius:70rpx;border:10rpx solid #fff;box-sizing:content-box;z-index:2048;height:70rpx;font-size:70rpx;width:70rpx;line-height:70rpx;text-align:center;box-shadow:0 -2px 8px rgba(255,255,255,0.15);color:#fff;background:var(--add-action);}.menuOpen{animation:menuO 0.3s both;}.menuClose{animation:menuC 0.3s both;}@keyframes menuO{0%{transform:rotate(0deg);color:#fff;background:var(--add-action);}100%{transform:rotate(45deg);color:#000;background:#fff;}}@keyframes menuC{0%{transform:rotate(45deg);color:#000;background:#fff;}100%{transform:rotate(-0deg);color:#fff;background:var(--add-action);}}.menu-container{display:flex;flex-direction:column;justify-content:center;align-items:center;position:fixed;height:100%;width:100%;z-index:2048;bottom:0rpx;}.menu{width:100%;flex:1;display:flex;flex-direction:column-reverse;padding-bottom:calc(134rpx + env(safe-area-inset-bottom));position:relative;}.menu-list{display:flex;align-items:center;flex-wrap:wrap;width:100%;padding-bottom:30rpx;}.menu-item{display:flex;flex-direction:column;align-items:center;animation:bounceInDown 0.45s linear both;}.menu-icon{width:110rpx;height:110rpx;margin-bottom:15rpx;border-radius:100%;}.menu-name{color:#607d8b;font-size:25rpx;font-weight:600;}@keyframes bounceInDown{0%{opacity:0;transform:translateY(100%);